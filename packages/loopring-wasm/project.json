{
  "name": "loopring-wasm",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/loopring-wasm",
  "implicitDependencies": [],
  "targets": {
    "wasm": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/loopring-wasm/**/*.{wasm}"],
      "options": {
        "cwd": "packages/loopring-wasm",
        "commands": [
          {
            "command": "mkdir -p ../../dist/packages/loopring-wasm && tinygo build --no-debug -o ../../dist/packages/loopring-wasm/wasm-release.wasm -target wasm",
            "forwardAllArgs": false
          }
        ]
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/loopring-wasm",
        "commands": [
          {
            "command": "go fmt ./...",
            "forwardAllArgs": false
          }
        ]
      }
    }
  },
  "tags": ["type:go"]
}
